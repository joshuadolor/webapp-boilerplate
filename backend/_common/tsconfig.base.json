{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES6",
        "module": "commonjs",
        "lib": [
            "ES6"
        ],
        "skipLibCheck": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true
    }
}
